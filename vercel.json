{
  "redirects": [
    {
      "source": "/",
      "destination": "/main/",
      "permanent": true,
     "missing": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*YandexBot.*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all, index, follow"
        }
      ]
    }
  ]
}
